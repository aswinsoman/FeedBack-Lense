{
  "name": "feedback-lense-e2e-tests",
  "version": "1.0.0",
  "description": "End-to-end tests for Feedback Lense application",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:f3": "playwright test e2e/f3",
    "test:f5": "playwright test e2e/f5",
    "test:f6": "playwright test e2e/f6",
    "test:f8": "playwright test e2e/f8",
    "test:f3:chrome": "playwright test e2e/f3 --project=chromium",
    "test:f5:chrome": "playwright test e2e/f5 --project=chromium",
    "test:f6:chrome": "playwright test e2e/f6 --project=chromium",
    "test:f8:chrome": "playwright test e2e/f8 --project=chromium",
    "test:f3:headed": "playwright test e2e/f3 --headed",
    "test:f5:headed": "playwright test e2e/f5 --headed",
    "test:f6:headed": "playwright test e2e/f6 --headed",
    "test:f8:headed": "playwright test e2e/f8 --headed",
    "test:f3:debug": "playwright test e2e/f3 --debug",
    "test:f5:debug": "playwright test e2e/f5 --debug",
    "test:f6:debug": "playwright test e2e/f6 --debug",
    "test:f8:debug": "playwright test e2e/f8 --debug",
    "test:chrome": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:safari": "playwright test --project=webkit",
    "test:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:report": "playwright show-report",
    "test:install": "playwright install",
    "test:install-deps": "playwright install-deps",
    "test:codegen": "playwright codegen",
    "test:trace": "playwright show-trace",
    "test:update-snapshots": "playwright test --update-snapshots",
    "test:retry": "playwright test --retries=3",
    "test:parallel": "playwright test --workers=4",
    "test:serial": "playwright test --workers=1",
    "test:grep": "playwright test --grep",
    "test:grep-invert": "playwright test --grep-invert",
    "test:timeout": "playwright test --timeout=60000",
    "test:max-failures": "playwright test --max-failures=5",
    "test:reporter": "playwright test --reporter=html,json,junit",
    "test:output": "playwright test --output=test-results",
    "test:config": "playwright test --config=playwright.config.js",
    "test:help": "playwright test --help",
    "test:version": "playwright --version",
    "test:list": "playwright test --list",
    "test:dry-run": "playwright test --dry-run",
    "test:reuse": "playwright test --reuse",
    "test:no-reuse": "playwright test --no-reuse",
    "test:headed-all": "playwright test --headed --project=chromium --project=firefox --project=webkit",
    "test:smoke": "playwright test --grep='smoke'",
    "test:regression": "playwright test --grep='regression'",
    "test:critical": "playwright test --grep='critical'",
    "test:feature": "playwright test --grep='feature'",
    "test:integration": "playwright test --grep='integration'",
    "test:unit": "playwright test --grep='unit'",
    "test:api": "playwright test --grep='api'",
    "test:ui": "playwright test --grep='ui'",
    "test:ux": "playwright test --grep='ux'",
    "test:accessibility": "playwright test --grep='accessibility'",
    "test:performance": "playwright test --grep='performance'",
    "test:security": "playwright test --grep='security'",
    "test:compatibility": "playwright test --grep='compatibility'",
    "test:mobile": "playwright test --grep='mobile'",
    "test:desktop": "playwright test --grep='desktop'",
    "test:tablet": "playwright test --grep='tablet'",
    "test:responsive": "playwright test --grep='responsive'",
    "test:cross-browser": "playwright test --project=chromium --project=firefox --project=webkit",
    "test:cross-platform": "playwright test --project=chromium --project=firefox --project=webkit --project='Mobile Chrome' --project='Mobile Safari'",
    "test:all": "playwright test --project=chromium --project=firefox --project=webkit --project='Mobile Chrome' --project='Mobile Safari'",
    "test:ci": "playwright test --project=chromium --project=firefox --project=webkit --reporter=html,json,junit --output=test-results",
    "test:local": "playwright test --project=chromium --headed --reporter=html",
    "test:dev": "playwright test --project=chromium --headed --reporter=html --workers=1",
    "test:staging": "playwright test --project=chromium --project=firefox --project=webkit --reporter=html,json",
    "test:production": "playwright test --project=chromium --project=firefox --project=webkit --reporter=html,json,junit --output=test-results",
    "test:nightly": "playwright test --project=chromium --project=firefox --project=webkit --project='Mobile Chrome' --project='Mobile Safari' --reporter=html,json,junit --output=test-results",
    "test:weekly": "playwright test --project=chromium --project=firefox --project=webkit --project='Mobile Chrome' --project='Mobile Safari' --reporter=html,json,junit --output=test-results --retries=2",
    "test:monthly": "playwright test --project=chromium --project=firefox --project=webkit --project='Mobile Chrome' --project='Mobile Safari' --reporter=html,json,junit --output=test-results --retries=3 --timeout=120000",
    "test:quarterly": "playwright test --project=chromium --project=firefox --project=webkit --project='Mobile Chrome' --project='Mobile Safari' --reporter=html,json,junit --output=test-results --retries=5 --timeout=300000",
    "test:yearly": "playwright test --project=chromium --project=firefox --project=webkit --project='Mobile Chrome' --project='Mobile Safari' --reporter=html,json,junit --output=test-results --retries=10 --timeout=600000",
    "test:custom": "playwright test --config=tests/playwright.config.js --project=chromium --headed --reporter=html --workers=1 --timeout=30000 --retries=1 --max-failures=3 --output=test-results --reuse --no-reuse --dry-run --list --help --version --grep --grep-invert --update-snapshots --debug --ui --headed --headed-all --smoke --regression --critical --feature --integration --unit --api --ui --ux --accessibility --performance --security --compatibility --mobile --desktop --tablet --responsive --cross-browser --cross-platform --all --ci --local --dev --staging --production --nightly --weekly --monthly --quarterly --yearly --custom"
  },
  "keywords": [
    "e2e",
    "testing",
    "playwright",
    "automation",
    "feedback-lense",
    "survey",
    "analytics",
    "pdf-export"
  ],
